<mvvm:BaseUserControl x:Class="DocumentPacker.Parts2.Header.Links.BackLinkPart.BackLinkView"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:dependencyProperties="clr-namespace:DocumentPacker.DependencyProperties"
                 xmlns:mvvm="clr-namespace:DocumentPacker.Mvvm">
   <Button
        Command="{Binding BackCommand}"
        dependencyProperties:ImageDependencyProperty.Image="../../../../Assets/material_symbol_arrow_back.png"
        dependencyProperties:TextDependencyProperty.Text="{Binding CommandText}"
        Style="{StaticResource NavigationBarLinkStyle}"/>
</mvvm:BaseUserControl>
