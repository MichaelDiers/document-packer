<!-- ReSharper disable Xaml.IndexOutOfGridDefinition -->
<mvvm:BaseUserControl
    x:Class="DocumentPacker.Parts.FooterPart.FooterView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mvvm="clr-namespace:DocumentPacker.Mvvm"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:footerPart="clr-namespace:DocumentPacker.Parts.FooterPart"
    xmlns:dependencyProperties="clr-namespace:Libs.Wpf.DependencyProperties;assembly=Libs.Wpf"
    mc:Ignorable="d"
    d:DataContext="{d:DesignInstance Type=footerPart:FooterViewModel, IsDesignTimeCreatable=False}">
    <Grid
        dependencyProperties:GridRowsDependencyProperty.GridRows="Auto"
        dependencyProperties:GridColumnsDependencyProperty.GridColumns="*|Auto">
        <TextBlock
            Grid.Row="0"
            Grid.Column="1"
            DataContext="{Binding Version}" />
    </Grid>
</mvvm:BaseUserControl>
<!-- ReSharper restore Xaml.IndexOutOfGridDefinition -->
