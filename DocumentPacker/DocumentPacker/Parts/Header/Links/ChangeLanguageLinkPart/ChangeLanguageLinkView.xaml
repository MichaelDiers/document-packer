<mvvm:BaseUserControl x:Class="DocumentPacker.Parts.Header.Links.ChangeLanguageLinkPart.ChangeLanguageLinkView"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:dependencyProperties="clr-namespace:DocumentPacker.DependencyProperties"
                 xmlns:mvvm="clr-namespace:DocumentPacker.Mvvm">
    <Button
        Command="{Binding RequestChangeLanguageViewCommand}"
        dependencyProperties:ImageDependencyProperty.Image="../../../../Assets/material_symbol_language.png"
        dependencyProperties:TextDependencyProperty.Text="{Binding CommandText}"/>
</mvvm:BaseUserControl>
