<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary
            Source="pack://application:,,,/Libs.Wpf;component/Styles/common/ComboBoxStyle.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style
        TargetType="ComboBox"
        BasedOn="{StaticResource TranslatableComboBoxStyle}">
        <Setter
            Property="Margin"
            Value="24" />
    </Style>

</ResourceDictionary>
