<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary
            Source="Basic.xaml" />
        <ResourceDictionary
            Source="WindowStyle.xaml" />
        <ResourceDictionary
            Source="SmallCommandButton.xaml" />
        <ResourceDictionary
            Source="pack://application:,,,/Libs.Wpf;component/Styles/CommandButtonStyle.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style
        TargetType="Button"
        BasedOn="{StaticResource CommandButtonStyle}" />
</ResourceDictionary>
