<Window x:Class="DocumentPacker.View.DocumentPackerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DocumentPacker.View"
        xmlns:viewModel="clr-namespace:DocumentPacker.ViewModel"
        mc:Ignorable="d"
        Icon="{Binding Icon, Mode=OneWay}"
        Title="{Binding Title, Mode=OneWay}" Height="450" Width="800">
    <Window.DataContext>
        <viewModel:DocumentPackerViewModel/>
    </Window.DataContext>
    <TextBlock Text="{Binding Version, Mode=OneTime}"/>
</Window>
