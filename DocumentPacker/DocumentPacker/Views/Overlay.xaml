<UserControl x:Class="DocumentPacker.Views.Overlay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModels="clr-namespace:DocumentPacker.ViewModels">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles/OverlayStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Border>
        <Grid>
            <Button Command="{Binding Path=(viewModels:TaskCommand.CancelCommand)}">
                Cancel
            </Button>
        </Grid>
    </Border>
</UserControl>